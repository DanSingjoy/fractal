{
  "name": "ordao-sys",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "config": {
    "respect1155": "../../../respect1155/sc/",
    "orec": "../../../orec/",
    "ordao_tests": "./",
    "ts_utils": "../../../ts-utils",
    "ortypes": "../../../ortypes",
    "orclient": "../../../orclient",
    "ornode": "../../../ornode",
    "ordao_gui": "../../gui",
    "ordao_console": "../../console",
    "ornode_to_cfg_path": "../ordao/sys/dev/tmp/ornode-dev-cfg.json"
  },
  "scripts": {
    "hh-test-ordao": "NODE_OPTIONS='--enable-source-maps --experimental-loader ts-node/esm/transpile-only --no-warnings=ExperimentalWarning' hardhat --show-stack-traces --network localhost test",
    "hh-dev-script": "NODE_OPTIONS='--experimental-loader ts-node/esm/transpile-only --no-warnings=ExperimentalWarning' hardhat --config ./hardhat-dev.config.cts --show-stack-traces --network localhost run",
    "hh-test-script": "NODE_OPTIONS='--experimental-loader ts-node/esm/transpile-only --no-warnings=ExperimentalWarning' hardhat --show-stack-traces --network localhost run",
    "hh-dev-chain": "npx hardhat --config ./hardhat-dev.config.cts node",
    "hh-test-chain": "npx hardhat --config ./hardhat.config.cts node",
    "test-deployment": "VOTE_PERIOD=86400 VETO_PERIOD=518400 npm run hh-test-script ./src/devDeployment.ts",
    "dev-deployment": "npm run hh-dev-script ./src/devDeployment.ts",
    "test-ordao": "node ./src/testOrdao.js",
    "testnet": "npx hardhat --config ./hardhat-dev.config.cts node",
    "watch-respect1155": "cd $npm_package_config_respect1155 && npm run build-watch -- --preserveWatchOutput",
    "build-respect1155": "cd $npm_package_config_respect1155 && npm run build",
    "build-respect1155-clean": "cd $npm_package_config_respect1155 && npm run build-clean",
    "test-respect1155": "cd $npm_package_config_respect1155 && npm run test",
    "setup-respect1155": "cd $npm_package_config_respect1155 && npm install && npm run build && npm run test",
    "watch-orec": "cd $npm_package_config_orec && npm run build-watch -- --preserveWatchOutput",
    "build-orec": "cd $npm_package_config_orec && npm run build",
    "build-orec-clean": "cd $npm_package_config_orec && npm run build-clean",
    "test-orec": "cd $npm_package_config_orec && npm run test",
    "setup-orec": "cd $npm_package_config_orec && npm install && npm run build && npm run test",
    "test": "npm run test-respect1155 && npm run test-orec && npm run test-ordao",
    "watch-ts-utils": "cd $npm_package_config_ts_utils && npm run build-watch -- --preserveWatchOutput",
    "build-ts-utils": "cd $npm_package_config_ts_utils && npm run build",
    "setup-ts-utils": "cd $npm_package_config_ts_utils && npm install && npm run build",
    "build-ts-utils-clean": "cd $npm_package_config_ts_utils && npm run build-clean",
    "watch-ortypes": "cd $npm_package_config_ortypes && npm run build-watch -- --preserveWatchOutput",
    "build-ortypes": "cd $npm_package_config_ortypes && npm run build",
    "setup-ortypes": "cd $npm_package_config_ortypes && npm install && npm run build",
    "build-ortypes-clean": "cd $npm_package_config_ortypes && npm run build-clean",
    "watch-orclient": "cd $npm_package_config_orclient && npm run build-watch -- --preserveWatchOutput",
    "build-orclient": "cd $npm_package_config_orclient && npm run build",
    "setup-orclient": "cd $npm_package_config_orclient && npm install && npm run build",
    "build-orclient-clean": "cd $npm_package_config_orclient && npm run build-clean",
    "watch-ornode": "cd $npm_package_config_ornode && npm run build-watch -- --preserveWatchOutput",
    "build-ornode": "cd $npm_package_config_ornode && npm run build",
    "setup-ornode": "cd $npm_package_config_ornode && npm install && npm run build",
    "setup-console": "cd $npm_package_config_ordao_console && npm install && npm run build",
    "build-console": "cd $npm_package_config_ordao_console && npm run build",
    "build-ornode-clean": "cd $npm_package_config_ornode && npm run build-clean",
    "build-watch": "concurrently --kill-others \"npm:watch-respect1155\" \"npm:watch-orec\" \"npm:watch-ts-utils\" \"npm:watch-ortypes\" \"npm:watch-orclient\" \"npm:watch-ornode\"",
    "init-db": "node src/setupMongodb.js",
    "dev-mongod": "node src/runDevMongod.js",
    "clean-dev-mongod": "rm -rf ./tmp/mongodb/dev/*",
    "dev-mongod-clean": "npm run clean-dev-mongod && npm run dev-mongod",
    "build": "npm run build-respect1155 && npm run build-orec && npm run build-ts-utils && npm run build-ortypes && npm run build-orclient && npm run build-ornode",
    "build-all": "npm run build-respect1155 && npm run build-orec && npm run build-ts-utils && npm run build-ortypes && npm run build-orclient && npm run build-ornode && npm run build-console",
    "build-clean": "npm run build-respect1155-clean && npm run build-orec-clean && npm run build-ts-utils-clean && npm run build-ortypes-clean && npm run build-orclient-clean && npm run build-ornode-clean",
    "setup-gui": "cd $npm_package_config_ordao_gui && npm install && npx vite build",
    "build-gui": "cd $npm_package_config_ordao_gui && npx vite build",
    "dev-gui": "node src/devGui.js",
    "dev-console": "node src/devConsole.js",
    "watch-orclient-docs": "node src/watchOrclientDocs.js",
    "dev-ornode": "node src/devOrnode.js",
    "dev-ornode-swagger-ui": "cd $npm_package_config_ornode && sleep 30 && ORNODE_CFG_PATH=$npm_package_config_ornode_to_cfg_path npm run swagger-ui",
    "dev-servers": "npm run build-clean && concurrently --kill-others \"npm:dev-ornode\" \"npm:dev-gui\" \"npm:dev-console\" \"npm:watch-*\" \"npm:dev-ornode-swagger-ui\"",
    "dev-servers-no-vite": "npm run build-clean && concurrently --kill-others \"npm:dev-ornode\" \"npm:watch-*\" \"npm:dev-ornode-swagger-ui\"",
    "test-contracts": "npm run test-orec && npm run test-respect1155",
    "dev": "node src/dev.js",
    "dev-no-vite": "node src/dev.js --no-vite",
    "setup": "npm install && npm run setup-orec && npm run setup-respect1155 && npm run setup-ts-utils && npm run setup-ortypes && npm run setup-orclient && npm run setup-ornode && npm run setup-gui && npm run setup-console"
  },
  "keywords": [],
  "author": "sim31",
  "license": "GPL-3.0",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@types/jsonfile": "^6.1.4",
    "@types/shelljs": "^0.8.15",
    "chai-as-promised": "^7.1.2",
    "concurrently": "^8.2.2",
    "hardhat": "^2.22.5",
    "jsonfile": "^6.1.0",
    "shelljs": "^0.8.5",
    "tree-kill": "^1.2.2",
    "typedoc": "^0.26.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "ethers": "^6.12.1",
    "ethers-decode-error": "^2.1.3",
    "mongodb": "^6.7.0",
    "op-fractal-sc": "github:Optimystics/op-fractal-sc#main",
    "orclient": "file:../../../orclient",
    "ornode": "file:../../../ornode",
    "ortypes": "file:../../../ortypes",
    "respect1155-sc": "file:../../../respect1155/sc",
    "ts-utils": "file:../../../ts-utils",
    "utility-types": "^3.11.0",
    "zod": "^3.23.8"
  }
}
